<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <ItemsPanelTemplate x:Key="WrapTemplate">
        <WrapPanel ItemWidth="300px" />
    </ItemsPanelTemplate>
    <Style x:Key="List" TargetType="ListBox">
        <Setter Property="Background" Value="Transparent" />
    </Style>
    <Style x:Key="MainGridBackground" TargetType="Grid">
        <Setter Property="Background" Value="#FFE6E6" />
    </Style>

    <Style x:Key="HabbitLabel" TargetType="Label">
        <Setter Property="FontSize" Value="15px" />
        <Setter Property="HorizontalAlignment" Value="Left" />
    </Style>
</ResourceDictionary>